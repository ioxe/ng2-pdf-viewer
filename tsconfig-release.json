{
    "compilerOptions": {
      "rootDir": "src/pdf-viewer",
      "outDir": "dist",
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "inlineSources": true,
      "declaration": true,
      "removeComments": true,
      "noImplicitAny": false,
      "lib": ["es6", "dom"],
      "typeRoots": [
        "node_modules/@types",
        "node_modules"
      ],
      "types": [
        "systemjs",
        "jasmine",
        "node",
        "pdf"
      ]
    },
    "files": [
      "src/pdf-viewer/index",
      "src/pdf-viewer/pdf-viewer.component.ts"
    ],
    "angularCompilerOptions": {
      "genDir": "aot",
      "strictMetadataEmit": true,
      "debug": true
    }
  }
  
  